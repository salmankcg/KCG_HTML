                  <div class="name">Eirik Lyng</div>
                  <div class="type">Social Media</div>
                </div>
              </div>
            </a>
            {% endfor %}

          </div>
        </div>
      </div>
    </div>
    <div class="button b-black b-icon b-center b-align-center">
      <span class="label">LOAD MORE</span>
      <div class="wrapper">
        <div class="arrow svg a-down"></div>
      </div>
    </div>
  </div>

</section>




{% endblock %}