{% extends "./layouts/main.njk" %}
{% set title        = 'KCG' %}
{% set page         = 'works' %}
{% set header       = '' %}
{% set footer       = '' %}


{% block content %}


<section>

  <div class="webdoor w-works">
    <div class="submenu">
      <div class="item active"><span>WORK</span></div>
    </div>

    <div class="container-fluid">
        <div class="row justify-content-center">
          <div class="col col-10">
            <h1 class="title t-medium t-center"><strong>Selected projects to show you<br>what we've been done</strong></h1>
          </div>
        </div>
    </div>
  </div>

  <div class="filter">
    <div class="container-fluid">
      <div class="row justify-content-center">
        <div class="col col-11">
